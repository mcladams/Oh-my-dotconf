[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[user]
	name = Michael C. Adams
	email = 10792116+mcladams@users.noreply.github.com
[pull]
	rebase = true
[fetch]
	prune = true
[diff]
	colorMoved = zebra
[rebase]
	autoSquash = true
[init]
	defaultbranch = main
[credential]
	helper = manager
[push]
	autoSetupRemote = true
[credential "helperselector"]
	selected = manager
